"Loose Methods"!

!BootstrapModalHeaderWebViewTest methodsFor!

testRenderContentOnWithComplexTitle	| header html |	header := BootstrapModalHeaderWebView		titled: [ :canvas | 			canvas				render: (Bootstrap3IconWebView representing: Glyphicons menuUp);				render: 'Up!!' ].	html := self render: header.	self		assert: html		equals: '<div class="modal-header"><h4 class="modal-title"><span class="glyphicon glyphicon-menu-up"></span>Up!!</h4></div>'! !
!BootstrapModalHeaderWebViewTest categoriesFor: #testRenderContentOnWithComplexTitle!*Willow/Bootstrap/3/Tests!public! !

!BootstrapPopoverTest methodsFor!

testDestroy	| html |	html := self renderPopoverContentApplying: [ :popover | popover destroy ].	self assert: html equals: '<div id="id1"></div><script type="text/javascript">$("#id1").popover("destroy");</script>'!

testTemplateForBootstrap3	| html |	html := self		renderUsing: [ :canvas | 			| div popover |			div := canvas div.			popover := canvas jQuery new bootstrapPopover.			popover				template: [ :template | 					template div						with: [ template div apply: [ :container | container addClass bootstrapCommon arrow ].							template								render:									( HeadingWebView										displaying: ''										withLevel: 3										applying: [ :container | container addClass bootstrap3 popoverTitle ] ).							template div								with: ( Bootstrap3IconWebView representing: Glyphicons menuUp )								applying: [ :container | container addClass bootstrap3 popoverContent ]							]						applying:							[ :container :constants | container addClass bootstrapCommon popover + ( container setARIARoleTo: #tooltip ) ]					].			div script: popover			].	self		assert: html		equals:			'<div id="id1"></div><script type="text/javascript">$("#id1").popover({"template":"<div class=\"popover\" role=\"tooltip\"><div class=\"arrow\"></div><h3 class=\"popover-title\"></h3><div class=\"popover-content\"><span class=\"glyphicon glyphicon-menu-up\"></span></div></div>"});</script>'!

testViewport	| html |	html := self renderPopoverContentApplying: [ :popover | popover viewport: '#id1' ].	self assert: html equals: '<div id="id1"></div><script type="text/javascript">$("#id1").popover({"viewport":"#id1"});</script>'!

testViewportWithPadding	| html |	html := self		renderPopoverContentApplying: [ :popover | popover viewport: (Bootstrap3ViewportConfiguration new selector: '#id1' padding: 5) ].	self		assert: html		equals: '<div id="id1"></div><script type="text/javascript">$("#id1").popover({"viewport":{"selector":"#id1","padding":5}});</script>'! !
!BootstrapPopoverTest categoriesFor: #testDestroy!*Willow/Bootstrap/3/Tests!public! !
!BootstrapPopoverTest categoriesFor: #testTemplateForBootstrap3!*Willow/Bootstrap/3/Tests!public! !
!BootstrapPopoverTest categoriesFor: #testViewport!*Willow/Bootstrap/3/Tests!public! !
!BootstrapPopoverTest categoriesFor: #testViewportWithPadding!*Willow/Bootstrap/3/Tests!public! !

!BootstrapTooltipTest methodsFor!

testTemplateForBootstrap3	| html |	html := self		renderUsing: [ :canvas | 			| div tooltip |			div := canvas div.			tooltip := canvas jQuery new bootstrapTooltip.			tooltip				template: [ :template | 					template div						with: [ template div apply: [ :container | container addClass bootstrap3 tooltipArrow ].							template div								with: ( Bootstrap3IconWebView representing: Glyphicons menuUp )								applying: [ :container | container addClass bootstrap3 tooltipInner ]							]						applying:							[ :container :constants | container addClass bootstrap3 tooltip + ( container setARIARoleTo: #tooltip ) ]					].			div script: tooltip			].	self		assert: html		equals:			'<div id="id1"></div><script type="text/javascript">$("#id1").tooltip({"template":"<div class=\"tooltip\" role=\"tooltip\"><div class=\"tooltip-arrow\"></div><div class=\"tooltip-inner\"><span class=\"glyphicon glyphicon-menu-up\"></span></div></div>"});</script>'!

testViewport	| html |	html := self renderTooltipContentApplying: [ :tooltip | tooltip viewport: '#id1' ].	self assert: html equals: '<div id="id1"></div><script type="text/javascript">$("#id1").tooltip({"viewport":"#id1"});</script>'!

testViewportWithPadding	| html |	html := self		renderTooltipContentApplying: [ :tooltip | tooltip viewport: (Bootstrap3ViewportConfiguration new selector: '#id1' padding: 5) ].	self		assert: html		equals: '<div id="id1"></div><script type="text/javascript">$("#id1").tooltip({"viewport":{"selector":"#id1","padding":5}});</script>'! !
!BootstrapTooltipTest categoriesFor: #testTemplateForBootstrap3!*Willow/Bootstrap/3/Tests!public! !
!BootstrapTooltipTest categoriesFor: #testViewport!*Willow/Bootstrap/3/Tests!public! !
!BootstrapTooltipTest categoriesFor: #testViewportWithPadding!*Willow/Bootstrap/3/Tests!public! !

