!BootstrapAlertCommand class methodsFor!

dark	^ self applying: [ :container | container addClass bootstrap4 alertDark ]!

light	^ self applying: [ :container | container addClass bootstrap4 alertLight ]!

primary	^ self applying: [ :container | container addClass bootstrap4 alertPrimary ]!

secondary	^ self applying: [ :container | container addClass bootstrap4 alertSecondary ]! !
